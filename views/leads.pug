doctype html
html(lang='en')
  include common/head.pug
  +head("All Leads")
  body
    if leads
       h1 Leads
       for lead in leads
           p 
               span #{lead.email}
               |
               a(href="/lead/" + lead.id) Details
    a(href="/") Back
  include common/footer.pug
  +footer()


